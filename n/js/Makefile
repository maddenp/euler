X=$(basename $(wildcard *.js))

.PHONY: test $(X)

test: $(X)

$(X):
	@test -n "$($@)" && (test "$$(node $@.js)" = "$($@)" && echo '$@ ok' || echo "$@ !! expected $($@), got $$(node $@.js)") || true
