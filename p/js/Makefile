X=$(basename $(wildcard *.js))

.PHONY: test $(X)

test: $(X)
	@echo

.ONESHELL:
$(X):
	@test -n "$(a$@)" && (test "$$(node $@.js)" = "$(a$@)" && echo -n '.' || echo "\nIn $@.js: expected=$(a$@), actual=$$(node $@.js)") || true
